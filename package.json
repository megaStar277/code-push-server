  "devDependencies": {
    "@shm-open/eslint-config-bundle": "1.6.0",
    "istanbul": "0.4.5",
    "mocha": "9.1.1",
    "should": "13.2.3",
    "standard-version": "9.3.1",
    "supertest": "6.1.6",
    "supervisor": "0.12.0"
  },
  "files": [
    "bin",
    "config",
    "core",
    "docs",
    "models",
    "public",
    "routes",
    "sql",
    "views",
    "app.js",
    "README.md",
    "LICENSE"
